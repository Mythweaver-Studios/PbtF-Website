// server/.env.example
# Server Port
PORT=3001

# Discord OAuth2 Credentials
# Get these from your application in the Discord Developer Portal
DISCORD_CLIENT_ID=YOUR_DISCORD_CLIENT_ID
DISCORD_CLIENT_SECRET=YOUR_DISCORD_CLIENT_SECRET

# The URI that Discord will redirect back to after authentication.
# This MUST match exactly what you've configured in the Discord Developer Portal.
# During development, it will point to a page on your React app.
DISCORD_REDIRECT_URI=http://localhost:3000/auth/callback 